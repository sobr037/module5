{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0",
        "title": "My User Project CRUD",
        "description": "My User Project Application API",
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        }
    },
    "host": "localhost:3000",
    "basePath": "/",
    "tags": [
        {
            "name": "Calculator",
            "description": "API for Calculus in the system"
        }
    ],
    "schemes": ["http"],
    "consumes": ["application/json"],
    "produces": ["application/json"],
    "paths": {
        "/calculator/add": {
            "get": {
                "tags": [
                    "Addition Service"
                ],
                "summary": "Add 2 numbers",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "description": "the First Number"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "description": "the Second number"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "This service allows you to add two numbers together"
                    }
                }
            }
        },
        "/calculator/minus": {
            "get": {
                "tags": [
                    "Subtraction Service"
                ],
                "summary": "Subtract 2 numbers",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "description": "the First Number"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "description": "the Second number"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "This service allows you to subtract two numbers"
                    }
                }
            }
        },
        "/calculator/times": {
            "get": {
                "tags": [
                    "Multiplication Service"
                ],
                "summary": "Multiplies 2 numbers",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "description": "the First Number"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "description": "the Second number"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "This service allows you to multiply two numbers"
                    }
                }
            }
        },
        "/calculator/divide": {
            "get": {
                "tags": [
                    "Division Service"
                ],
                "summary": "Divides 2 numbers",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "description": "the First Number"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "description": "the Second number"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "This service allows you to divide two numbers"
                    }
                }
            }
        },
        "/calculator/percentage": {
            "get": {
                "tags": [
                    "Percentage Service"
                ],
                "summary": "Returns a certain percentage of a specified number",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "description": "Percentage your wanting to determine"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "description": "Number your wanting to determine the percentage of"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "This service allows you to determine a specific percentage of a number"
                    }
                }
            }
        }
    }
}
